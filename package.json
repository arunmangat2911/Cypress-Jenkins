{
  "name": "cypress-course",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "test-electron": "cypress run --spec \"cypress//e2e//Tc01_FirstScript.spec.cy.js\"",
    "smoke-test": "cypress run --spec \"cypress/e2e/smokeTest/*.cy.js\"",
    "test-electron-headed": "cypress run --headed",
    "test-chrome": "cypress run --browser chrome",
    "test-chrome-headed": "cypress run --browser chrome --headed",
    "test-dashboard": "npx cypress run --record --key dd57ebdc-549e-49e8-89f8-986ba7db6d82",
    "test-smoke": "cypress run --spec \"cypress/e2e/smokeTest/*.js\"",
    "scripts":"cypress run --spec \"cypress/e2e/smokeTest//*.js\"",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cypress": "^14.0.3"
  },
  "devDependencies": {
    "cypress-multi-reporters": "^2.0.5",
    "cypress-xpath": "^2.0.1",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "neat-csv": "^5.2.0",
    "xlsx": "^0.18.5"
  }
}
